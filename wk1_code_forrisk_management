      "cell_type": "code",
      "source": [
        "# Fetch Historical Stock Prices\n",
        "def fetch_stock_data(tickers, start_date=\"2015-01-01\", end_date=\"2024-01-01\"):\n",
        "    data = yf.download(tickers, start=start_date, end=end_date)[\"Close\"]\n",
        "    return data"
      ],
      "metadata": {
        "id": "JGJJS2FVpRof"
      },
      "id": "JGJJS2FVpRof",
      "execution_count": 15,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# Stop Loss Function: Stop trading if the spread exceeds a threshold\n",
        "def stop_loss(spread, threshold=2):\n",
        "    return spread > threshold  # Exiting position if spread exceeds threshold"
      ],
      "metadata": {
        "id": "nZ8ZEutHpRlE"
      },
      "id": "nZ8ZEutHpRlE",
      "execution_count": 16,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# Position Sizing Based on Volatility: Larger position sizes for lower volatility\n",
        "def position_sizing(spread, risk_factor=0.10):  # Increased risk factor to 0.10\n",
        "    # Convert spread to pandas Series if it's numpy ndarray\n",
        "    spread = pd.Series(spread)\n",
        "\n",
        "    # Compute the volatility of the spread (Rolling Standard Deviation)\n",
        "    spread_volatility = spread.rolling(window=30).std()\n",
        "\n",
        "    # Calculate position size based on risk factor and volatility\n",
        "    position_size = risk_factor / spread_volatility\n",
        "    return position_size.fillna(0)  # Fill NaNs for the initial period"
      ],
      "metadata": {
        "id": "GzFpaGNApVtJ"
      },
      "id": "GzFpaGNApVtJ",
      "execution_count": 17,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# Volatility Threshold: Enter trade only when volatility exceeds a threshold\n",
        "def volatility_threshold(spread, threshold=0.02):  # Decreased volatility threshold to 0.02\n",
        "    spread_volatility = pd.Series(spread).rolling(window=30).std()\n",
        "    return spread_volatility > threshold"
      ],
      "metadata": {
        "id": "6Plwzio0pX0-"
      },
      "id": "6Plwzio0pX0-",
      "execution_count": 18,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# Backtest with Risk Management: Combine all techniques into a backtest\n",
        "def backtest_with_risk_management(data, stock1, stock2, stop_loss_threshold=2, risk_factor=0.10, volatility_threshold_value=0.02):\n",
        "    # Calculate Spread between two stocks\n",
        "    spread = data[stock2] - data[stock1]\n",
        "\n",
        "    # Apply stop-loss rule\n",
        "    stop_loss_flag = stop_loss(spread, stop_loss_threshold)\n",
        "\n",
        "    # Apply volatility threshold rule\n",
        "    volatility_flag = volatility_threshold(spread, volatility_threshold_value)\n",
        "\n",
        "    # Calculate position sizes(manually multiplied by 10000 to scale everything up, and thus we have a higher capital to work with)\n",
        "    position_size = position_sizing(spread, risk_factor)*10000\n",
        "\n",
        "    # Compute daily returns and backtest logic\n",
        "    daily_returns = spread.pct_change().shift(-1).fillna(0)\n",
        "\n",
        "    # Initialize position (always buy or sell 10000, thus indicating a higher capital)\n",
        "    position = np.zeros(len(spread))*10000\n",
        "\n",
        "    # When volatility exceeds threshold, we enter the trade\n",
        "    position[volatility_flag] = position_size[volatility_flag]\n",
        "\n",
        "    # Stop loss condition: Exiting the position if stop-loss is triggered\n",
        "    position[stop_loss_flag] = 0\n",
        "\n",
        "    # Compute PnL\n",
        "    pnl = position * daily_returns\n",
        "    cumulative_pnl = pnl.cumsum()\n",
        "\n",
        "    return cumulative_pnl, pnl"
      ],
      "metadata": {
        "id": "DoKzbdAxpZ7B"
      },
      "id": "DoKzbdAxpZ7B",
      "execution_count": 19,
      "outputs": []
    },
    {
      "cell_type": "code",
      "execution_count": 20,
      "metadata": {
        "id": "m0pMa3M3koB9"
      },
      "outputs": [],
      "source": [
        "# Backtest without Risk Management (non-risk-managed strategy) (used for comparing)\n",
        "def backtest_without_risk_management(data, stock1, stock2):\n",
        "    # Calculate Spread between two stocks\n",
        "    spread = data[stock2] - data[stock1]\n",
        "\n",
        "    # Compute daily returns and backtest logic\n",
        "    daily_returns = spread.pct_change().shift(-1).fillna(0)\n",
        "\n",
        "    # Position size is always 10000\n",
        "    position = np.ones(len(spread))*10000\n",
        "\n",
        "    # Compute PnL for non-risk-managed strategy\n",
        "    pnl = position * daily_returns\n",
        "    cumulative_pnl = pnl.cumsum()\n",
        "\n",
        "    return cumulative_pnl, pnl"
      ],
      "id": "m0pMa3M3koB9"
    },
    {
      "cell_type": "code",
      "source": [
        "# Run the simulation for a sample pair of stocks\n",
        "tickers = [\"MSFT\", \"AAPL\"]\n",
        "data = fetch_stock_data(tickers)\n",
        "\n",
        "# Run backtest with risk management techniques (increase risk factor and volatility threshold)\n",
        "cumulative_pnl_rm, pnl_rm = backtest_with_risk_management(data, 'MSFT', 'AAPL', stop_loss_threshold=2, risk_factor=0.10, volatility_threshold_value=0.02)\n",
        "\n",
        "# Run backtest without risk management\n",
        "cumulative_pnl_non_rm, pnl_non_rm = backtest_without_risk_management(data, 'MSFT', 'AAPL')"
      ],
      "metadata": {
